            <children>
               <Label prefHeight="15.0" prefWidth="90.0" text="Date d'arrivée" />
               <Pane prefHeight="9.0" prefWidth="142.0" />
               <DatePicker />
            </children>
         </VBox>
         <Pane prefHeight="72.0" prefWidth="41.0" />
