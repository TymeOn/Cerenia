            <children>
               <Pane prefHeight="24.0" prefWidth="100.0" />
               <Button mnemonicParsing="false" prefHeight="0.0" prefWidth="55.0" text="Login" />
            </children>
         </VBox>
      </children>
   </HBox>
   <ScrollPane fx:id="mainPane" hbarPolicy="NEVER" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
