            </children>
         </VBox>
         <Pane prefHeight="72.0" prefWidth="0.0" />
         <VBox alignment="CENTER_LEFT" prefHeight="6.0" prefWidth="100.0">
            <children>
               <Pane prefHeight="25.0" prefWidth="100.0" />
