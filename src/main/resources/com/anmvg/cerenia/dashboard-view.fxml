            </children>
         </VBox>
         <Pane prefHeight="72.0" prefWidth="230.0" />
         <VBox alignment="CENTER_LEFT" prefHeight="72.0" prefWidth="133.0">
            <children>
               <Label text="Destination" />
               <Pane prefHeight="12.0" prefWidth="87.0" />
               <TextField prefHeight="23.0" prefWidth="102.0" />
            </children>
         </VBox>
         <Pane prefHeight="72.0" prefWidth="40.0" />
         <VBox alignment="CENTER_LEFT" prefHeight="51.0" prefWidth="142.0">
            <children>
               <Label prefHeight="15.0" prefWidth="96.0" text="Date de départ" />
